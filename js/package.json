{
  "name": "galaxylab",
  "version": "0.1.0",
  "description": "A Custom Jupyter Widget Library",
  "author": "jayadev joshi",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/galaxy/galaxylab.git"
  },
  "keywords": [
    "jupyter",
    "widgets",
    "ipython",
    "ipywidgets",
    "jupyterlab-extension"
  ],
  "files": [
    "lib/**/*.js",
    "dist/*.js"
  ],
  "scripts": {
    "clean": "rimraf dist/ && rimraf ../galaxylab/labextension/ && rimraf ../galaxylab/nbextension",
    "prepublish": "yarn run clean && yarn run build:prod",
    "build": "webpack --mode=development && yarn run build:labextension:dev",
    "build:prod": "webpack --mode=production && yarn run build:labextension",
    "build:labextension": "jupyter labextension build .",
    "build:labextension:dev": "jupyter labextension build --development True .",
    "watch": "webpack --watch --mode=development",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "@jupyterlab/builder": "^3.0.0",
    "rimraf": "^2.6.1",
    "webpack": "^5.21.2",
    "webpack-cli": "^4.7.2"
  },
  "dependencies": {
    "@genepattern/nbtools": "file:../../newnbtools/nbtools",
    "@jupyter-widgets/base": "^1.1 || ^2 || ^3 || ^4",
    "@jupyterlab/notebook": "^3.1.0",
    "blueimp-file-upload": "^10.31.0",
    "css-loader": "^6.2.0",
    "file-loader": "^6.2.0",
    "lodash": "^4.17.4",
    "raw-loader": "^4.0.2",
    "source-map-loader": "^2.0.1",
    "style-loader": "^3.2.1"
  },
  "jupyterlab": {
    "extension": "lib/plugin",
    "outputDir": "../galaxylab/labextension",
    "sharedPackages": {
      "@jupyter-widgets/base": {
        "bundled": false,
        "singleton": true
      }
    }
  }
}
